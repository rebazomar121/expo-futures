{
  "expo": {
    "name": "testFutureExpo",
    "slug": "testFutureExpo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": ["CAMERA", "CAMERA_ROLL"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  },
  "plugins": [
    [
      "expo-camera",
      {
        "cameraPermission": "Allow Tester to access your camera."
      },
      "expo-barcode-scanner",
      {
        "cameraPermission": "Allow Tester to access your camera."
      },
      "expo-local-authentication",
      {
        "faceIDPermission": "Allow Tester to use Face ID."
      }
    ]
  ]
}
